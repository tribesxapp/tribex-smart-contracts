    "verify-coin-flip-supra": "truffle run verify CoinFlipGameSupra --license MIT --network",
    "play-coin-flip": "truffle exec scripts/play-coin-flip.js --network",
    "play-dice": "truffle exec scripts/play-dice.js --network",
    "send-tokens": "truffle exec scripts/send-tokens.js --network",
